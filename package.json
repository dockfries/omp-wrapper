{
  "name": "omp-wrapper",
  "version": "0.2.0",
  "description": "A functional wrapper of the omp new scripting api (natives and callbacks) for samp-node.",
  "main": "dist/bundle.js",
  "types": "dist/bundle.d.ts",
  "scripts": {
    "lint": "eslint --fix ./src",
    "build": "npm run lint && rollup -c",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yucarl77/omp-wrapper.git"
  },
  "keywords": [
    "samp",
    "sa:mp",
    "sa-mp",
    "omp",
    "open.mp",
    "pawn",
    "wrapper"
  ],
  "prepare": "npm run build",
  "author": "YuCarl77",
  "license": "ISC",
  "devDependencies": {
    "@sa-mp/node": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "esbuild": "^0.15.18",
    "eslint": "^8.31.0",
    "husky": "^8.0.2",
    "rollup": "^3.9.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.1.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^4.9.4"
  },
  "bugs": {
    "url": "https://github.com/yucarl77/omp-wrapper/issues"
  },
  "homepage": "https://github.com/yucarl77/omp-wrapper#readme"
}
